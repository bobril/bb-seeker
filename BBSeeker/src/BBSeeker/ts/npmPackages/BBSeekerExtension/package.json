{
  "name": "bbseeker-extended",
  "version": "2.1.17",
  "description": "Library providing tools for emulating user actions in front-end applications built by Bobril javascript framework.",
  "author": {
    "name": "p.sulc"
  },
  "dependencies": {
    "bbseeker": "*",
    "bobril": "*"
  },
  "main": "lib.ts",
  "publishConfig": {
    "registry": "https://pkgs.dev.azure.com/storeit/_packaging/bobwai-npm/npm/registry/"
  },
  "bobril": {
    "dependencies": "upgrade",
    "example": "examples",
    "compilerOptions": {
      "noImplicitAny": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "strictNullChecks": true,
      "declaration": true,
      "strictFunctionTypes": true,
      "strictPropertyInitialization": true
    }
  }
}
